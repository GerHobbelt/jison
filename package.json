{
  "author": "Zach Carter <zach@carter.name> (http://zaa.ch)",
  "contributors": [ "russa" ],
  "name": "jison",
  "description": "A parser generator with Bison's API",
  "version": "0.4.18-mre.1",
  "license": "MIT",
  "keywords": [
    "jison",
    "bison",
    "yacc",
    "parser",
    "generator",
    "lexer",
    "flex",
    "tokenizer",
    "compiler"
  ],
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/mmig/jison.git"
  },
  "bugs": {
    "url": "http://github.com/mmig/jison/issues"
  },
  "main": "lib/jison",
  "bin": "lib/cli.js",
  "engines": {
    "node": ">=0.4"
  },
  "dependencies": {
    "JSONSelect": "0.4.0",
    "cjson": "0.3.0",
    "ebnf-parser": "0.1.10",
    "escodegen": "1.3.x",
    "esprima": "1.1.x",
    "lex-parser": "~0.1.3",
    "nomnom": "1.5.2"
  },
  "devDependencies": {
    "browserify": "^16.5.1",
    "jison": "0.4.x",
    "test": "0.6.x",
    "uglify-js": "^3.9.3"
  },
  "scripts": {
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "browserify entry.js --exports require --outfile dist/jison.js",
    "build:prod": "uglifyjs dist/jison.js --output dist/jison.min.js",
    "test": "node tests/all-tests.js"
  },
  "homepage": "http://jison.org"
}
