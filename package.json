{
  "author": {
    "name": "Eric Prudhommeaux",
    "email": "eric@w3.org",
    "url": "http://www.w3.org/People/Eric/"
  },
  "name": "ts-jison",
  "description": "A lightly-typescriptified version of jison",
  "version": "0.0.5",
  "license": "MIT",
  "keywords": [
    "jison",
    "bison",
    "yacc",
    "parser",
    "generator",
    "lexer",
    "flex",
    "tokenizer",
    "compiler",
    "typescript"
  ],
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/ericprud/ts-jison.git"
  },
  "bugs": {
    "url": "http://github.com/ericprud/ts-jison/issues"
  },
  "engines": {
    "node": ">=0.4"
  },
  "dependencies": {
    "@types/node": "^14.14.25",
    "cjson": "0.3.0",
    "ebnf-parser": "0.1.10",
    "escodegen": "1.3.x",
    "esprima": "1.1.x",
    "JSONSelect": "0.4.0",
    "lex-parser": "~0.1.3",
    "nomnom": "1.5.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "browserify": "2.x.x",
    "jison": "0.4.x",
    "test": "0.6.x",
    "uglify-js": "~2.4.0"
  },
  "scripts": {
    "calculator": "npm run generate:calculator && npm run compile:calculator && npm run run:calculator && npm run ts-node:calculator",
    "generate:calculator": "./packages/parser-generator/lib/cli.js -t typescript -o packages/parser-generator/examples/ts-calculator.ts packages/parser-generator/examples/ts-calculator.jison",
    "compile:calculator": "tsc packages/parser-generator/examples/ts-calculator",
    "run:calculator": "node packages/parser-generator/examples/ts-calculator.js packages/parser-generator/examples/testcalc",
    "ts-node:calculator": "ts-node packages/parser-generator/examples/ts-calculator.ts packages/parser-generator/examples/testcalc",
    "test": "node tests/all-tests.js"
  },
  "homepage": "http://jison.org"
}
